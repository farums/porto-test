{
    "name": "packages/mezzio-core-porto",
    "type": "library",
    "version": "1.0.0",
    "description": "core",
    "homepage": "https://github.com/pagination",
    "license": "BSD-2-Clause",
    "keywords": [
        "porto"
    ],
    "require": {
        "php": "~8.1.0",
        "eventsauce/eventsauce": "^3.4",
        "laminas/laminas-config-aggregator": "^1.14",
        "laminas/laminas-servicemanager": "^3.21",
        "league/tactician": "^1.1",
        "mezzio/mezzio-problem-details": "^1.12",
        "monolog/monolog": "^3.4",
        "predis/predis": "^2.2",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3.5",
        "symfony/var-dumper": "^v6.3.5"
    },
    "autoload": {
        "psr-4": {
            "Ship\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShipTest\\": "Tests/",
            "PHPUnit\\Metadata\\": "vendor/phpunit/phpunit/src/Metadata",
            "PHPUnit\\Metadata\\Annotation\\Parser\\": "vendor/phpunit/phpunit/src/Metadata/Parser/Annotation"
        }
    },
    "scripts": {
        "phpunit": "phpunit --colors=always"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}